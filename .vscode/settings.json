{
  "files.exclude": {
    "**/*.csproj.user": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.idea": true,
    "**/.sonarlint": true,
    "**/.vs": true,
    "**/StyleCop.Cache": true,
    "**/bin": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/obj": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/bazel-out/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true
  },
  "csharp.semanticHighlighting.enabled": true,
}
